			<CARREGAR>10.1.3 Matriculas Geradas</CARREGAR>
			<LEGENDA>Contrato/familias Geradas</LEGENDA>
			<SQLESPECIAL>(
EXISTS(SELECT 1
                 FROM SAM_ROTINAIMP_BENEF X 
              WHERE (X.SITUACAO = 'G')
                    AND (X.IMPFAM = A.HANDLE)
                    AND (X.MOVIMENTACAO = 'I' OR X.MOVIMENTACAO IS NULL)) 
OR
EXISTS(SELECT 1
               FROM SAM_ROTINAIMP_FAM X
              WHERE X.HANDLE = A.HANDLE
                   AND (X.MOVIMENTACAO = 'I' OR X.MOVIMENTACAO IS NULL)
                   AND X.SITUACAO = 'G' 
                   AND X.MATRICULAFUNCIONAL IS NOT NULL)
)</SQLESPECIAL>
