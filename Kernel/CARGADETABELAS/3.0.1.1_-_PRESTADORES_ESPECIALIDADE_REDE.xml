			<LEGENDA>Prestadores</LEGENDA>
			<SQLESPECIAL>A.HANDLE IN (SELECT E.PRESTADOR 
                            FROM SAM_PRESTADOR_ESPECIALIDADE        E
                              JOIN SAM_PRESTADOR_ESPECIALIDADEGRP G ON (G.PRESTADORESPECIALIDADE = E.HANDLE)
                              JOIN SAM_PRESTADOR_ESPEC_GRP_REDE R ON (R.PRESTADORESPECIALIDADEGRUPO = G.HANDLE)
                         WHERE E.ESPECIALIDADE = @TABELA|SAM_ESPECIALIDADE
                               AND E.DATAINICIAL &lt;= @TODAY
                              AND (E.DATAFINAL    &gt;= @TODAY OR E.DATAFINAL IS NULL)
                              AND R.REDE = @TABELA|SAM_REDERESTRITA
